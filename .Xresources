! -- Terminal  -- !
Rxvt*loginShell:  true
Rxvt*background:  Black
Rxvt*foreground:  White
rxvt*title:                urxvt
rxvt*termName:             rxvt-256color

! -- Font -- !
URxvt.font: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
!rxvt*boldFont: -*-terminus-medium-*-*-*-*-140-*-*-*-*-iso8859-1
rxvt*cursorColor: #5555AA
!  - pseudo transparency
!Urxvt*shading:    50
!Urxvt*transparent:true


! -- Extensions -- !
urxvt*perl-lib:        /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher,searchable-scrollback
urxvt*urlLauncher:     /usr/bin/firefox
urxvt*matcher.button:  1
!  - catch ugly URLs
urxvt.cutchars:        `()'*<>[]{|}"

! -- Colors -- !
Rxvt*color0:  #000000 // black
Rxvt*color1:  #B40000 // red
Rxvt*color2:  #00AA00 // green
Rxvt*color3:  #AAAA00 // yellow
Rxvt*color4:  #5555AA // blue
Rxvt*color5:  #AA00AA // magenta
Rxvt*color6:  #00AAAA // cyan
Rxvt*color7:  #AAAAAA // white
Rxvt*color8:  #555555 // black (now and below: bright)
Rxvt*color9:  #FF0000 // red
Rxvt*color10: #00FF00 // green
Rxvt*color11: #FFFF00 // yellow
Rxvt*color12: #6464FF // blue
Rxvt*color13: #FF00FF // magenta
Rxvt*color14: #00FFFF // cyan
Rxvt*color15: #FFFFFF // whiteex
